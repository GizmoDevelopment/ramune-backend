{
	"name": "functions",
	"version": "1.2.0",
	"scripts": {
		"prepare": "npx ts-patch install -s",
		"lint": "eslint src",
		"build": "rm -rf ./lib && tsc",
		"serve": "npm run build && firebase emulators:start --only functions",
		"shell": "npm run build && firebase functions:shell",
		"start": "npm run shell",
		"deploy": "firebase deploy --only functions",
		"logs": "firebase functions:log"
	},
	"engines": {
		"node": "14"
	},
	"os": [
		"!win32"
	],
	"main": "lib/index.js",
	"dependencies": {
		"@google-cloud/firestore": "^5.0.2",
		"cors": "^2.8.5",
		"express": "^4.18.1",
		"firebase-admin": "^11.0.1",
		"firebase-functions": "^3.22.0",
		"iso-639-1": "^2.1.15"
	},
	"devDependencies": {
		"@gizmo-dev/eslint-plugin": "^3.2.0",
		"@types/cors": "^2.8.12",
		"@types/node": "^14.18.24",
		"eslint": "^8.22.0",
		"firebase-functions-test": "^2.3.0",
		"ts-patch": "^2.0.2",
		"typescript": "^4.7.4",
		"typescript-transform-paths": "^3.3.1"
	},
	"private": true
}
